<form method="post" action="{{ path('app_back_dog_delete', {'id': dog.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer ce chien ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dog.id) }}">
    <button class="btn btn-danger m-1 mx-0">Supprimer</button>
</form>
